<p>
  <label for="<%= f.object_name %>_name">Name</label><br/>
  <%= f.text_field :name %>
</p>
<p>
  <label for="<%= f.object_name %>_location_name">Location Name (name of Hotel or Conference Center)</label><br/>
  <%= f.text_field :location_name %>
</p>
<p>
  <label for="<%= f.object_name %>_address">Address</label><br/>
  <%= f.text_field :address %>
</p>
<p>
  <label for="<%= f.object_name %>_city">City</label><br/>
  <%= f.text_field :city %>
</p>
<p>
  <label for="<%= f.object_name %>_state">State</label><br/>
  <%= f.us_state_select :state %>
</p>
<p>
  <label for="<%= f.object_name %>_zip">Zip</label><br/>
  <%= f.text_field :zip %>
</p>

<p>
  <label for="<%= f.object_name %>_start_date">Start Date</label><br/>
  <%= f.datetime_select :start_date %>
</p>

<p>
  <label for="<%= f.object_name %>_end_date">End Date</label><br/>
  <%= f.datetime_select :end_date %>
</p>

<p>
  <label for="<%= f.object_name %>_description">Description</label><br/>
  <%= fckeditor_textarea("conference", "description", :toolbarSet => 'Jasper', :width => '100%', :height => '400px') %>
</p>
